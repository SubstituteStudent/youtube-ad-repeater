{
    "name": "AdReapeater",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "128": "/images/icon.png"
    },
    "action": {
        "default_popup": "/popup.html",
        "default_icon": "/images/icon.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.youtube.com/*"],
            "css": [],
            "js": ["content.js"]
        }
    ]
}
